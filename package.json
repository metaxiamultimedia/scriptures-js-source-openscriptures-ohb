{
  "name": "@metaxia/scriptures-source-openscriptures-ohb",
  "version": "2.0.0",
  "description": "Open Hebrew Bible (Hebrew) data for @metaxia/scriptures. Source: Open Scriptures Project.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    },
    "./metadata": {
      "types": "./data/openscriptures-OHB/metadata.json",
      "import": "./data/openscriptures-OHB/metadata.json"
    },
    "./data/*": "./data/*",
    "./cache/*": "./cache/*",
    "./books/*": {
      "types": "./dist/books/*.d.ts",
      "import": "./dist/books/*.js"
    }
  },
  "files": [
    "dist",
    "data",
    "cache"
  ],
  "scripts": {
    "build": "tsc",
    "import": "tsx scripts/import.ts",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bible",
    "hebrew",
    "hebrew-bible",
    "scriptures",
    "openscriptures",
    "morphology"
  ],
  "author": "Benjamin Plunkert",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-openscriptures-ohb"
  },
  "bugs": {
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-openscriptures-ohb/issues"
  },
  "homepage": "https://github.com/metaxiamultimedia/scriptures-js-source-openscriptures-ohb#readme",
  "peerDependencies": {
    "@metaxia/scriptures-core": "^2.0.0"
  },
  "devDependencies": {
    "@metaxia/scriptures-core": "^2.0.0",
    "@types/node": "^22.0.0",
    "fast-xml-parser": "^4.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.15"
  }
}
